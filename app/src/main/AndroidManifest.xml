<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="no.hiof.kjorbar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.StartNewCalculationActivity" />
        <activity android:name=".activities.SetLimitActivity" />
        <activity android:name=".activities.PreviousCalculations" />
        <activity android:name=".activities.CalculationActivity" />
        <activity android:name=".activities.AddUnitActivity" />
        <activity android:name=".activities.CalculateActivity" />
        <activity android:name=".activities.ProfileActivity" />
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>